{
  "expo": {
    "name": "uniza-parking",
    "slug": "uniza-parking",
    "extra": {
      "eas": {
        "projectId": "1db89bb2-8e09-4954-bec7-39e75d67db34"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/uniza-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/uniza-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/uniza-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "unizaparking"
  }
}
